{
  "env": {
    "node": true,
    "es6": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 2020
  },
  "rules": {
    "no-undef": "error",
    "no-unused-vars": "warn",
    "semi": ["error", "always"],
    "quotes": ["warn", "double"],
    "indent": ["warn", 2],
    "comma-dangle": ["warn", "always-multiline"],
    "no-console": "off"
  }
}
